#page-p-home .seperator {
     height: 0.82em;
     visibility: hidden;
}

#page-p-home #main-streamdata {
     background: none repeat scroll 0% 0% transparent;
     box-shadow: none;
}

#page-p-home .info {
     background: none repeat scroll 0% 0% transparent;
     box-shadow: none;
}

#page-p-home #introcontainer {
     background: none repeat scroll 0% 0% transparent;
     box-shadow: none;
}

#page-p-home #nowplaying {
