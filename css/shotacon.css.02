#page-p-home .news .border {
     background: none repeat scroll 0% 0% #F7F7F7;
     border-radius: 0px 0px 12px 12px;
     border: medium none;
     box-shadow: 0px 2px 8px rgba(254, 133, 52, 0.5);
}

#page-p-home .news h5 a {
     border-bottom: 1px solid #CCCCCC ! important;
     border-radius: 12px 12px 0px 0px ! important;
     color: #FFFFFF ! important;
     font-weight: normal ! important;
     letter-spacing: 0px ! important;
     text-shadow: 0px 1px 0px #000000 ! important;
}

#page-p-home .news h5 a:hover {
     background: none repeat scroll 0% 0% transparent ! important;
     color: #FFFFFF ! important;
     text-shadow: 0px 1px 0px #FFFFFF;
}

#page-p-home .progress {
     background: none repeat scroll 0px 0px #FFFFFF ! important;
     box-shadow: 0px 0px 1px 1px rgba(0, 0, 0, 0.2) inset;
}

#page-p-home .progress .bar {
     background: none repeat scroll 0px 0px #A6D42B ! important;
}

#page-p-home #progress .progress, #progress .bar {
     border-radius: 1px 1px 1px 1px;
}

#page-p-home .djmedia {
     background: none repeat scroll 0% 0% rgba(255, 255, 255, 0.7);
     border-radius: 6px 6px 6px 6px ! important;
     border: medium none ! important;
     box-shadow: 0px 1px 8px rgba(254, 133, 52, 0.45) ! important;
     position: relative;
     z-index: 9001;
}

#page-p-home #last-played-container td {
     -moz-transition: all 0.3s ease-in-out 0s ;
     -webkit-transition: all 0.3s ease-in-out 0s ;
     -o-transition: all 0.3s ease-in-out 0s ;
     -ms-transition: all 0.3s ease-in-out 0s ;
     transition: all 0.3s ease-in-out 0s ;
}

#page-p-home #queue-container td {
     -moz-transition: all 0.3s ease-in-out 0s ;
     -webkit-transition: all 0.3s ease-in-out 0s ;
     -o-transition: all 0.3s ease-in-out 0s ;
     -ms-transition: all 0.3s ease-in-out 0s ;
     transition: all 0.3s ease-in-out 0s ;
}

#page-p-home #last-played-container td:hover {
     color: #000000;
     padding-left: 2em ! important;
}

#page-p-home #queue-container td:hover {
     color: #000000;
     padding-left: 2em ! important;
}

#streamconnect-panel {
     background: none repeat scroll 0% 0% transparent ! important;
     border: medium none ! important;
     box-shadow: none ! important;
     display: none;
     float: left;
     margin-bottom: 0%;
     margin-top: -1.4em;
     opacity: 0;
     padding: 5px;
     position: relative;
     width: 528px;
     z-index: 5;
}