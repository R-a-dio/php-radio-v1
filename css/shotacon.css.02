html {
     background-attachment: fixed;
     background-clip: border-box;
     background-color: #FFFFFF;
     background-image: url("shotaconBG.jpg");
     background-origin: padding-box;
     background-position: center top;
     background-repeat: no-repeat;
     background-size: cover;
     min-height: 100%;
}

body {
     background: none repeat scroll 0% 0% transparent;
}

#page-p-home .content-top {
	background: -moz-linear-gradient(top,  rgba(255,255,255,1) 0%, rgba(255,255,255,0.85) 128px); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(128px,rgba(255,255,255,0.85))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.85) 128px); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.85) 128px); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.85) 128px); /* IE10+ */
	background: linear-gradient(to bottom,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.85) 128px); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#e6ffffff',GradientType=0 ); /* IE6-9 */
     border-radius: 0px 0px 10px 10px;
     box-shadow: 0px 1px 12px 4px rgba(135, 113, 214, 0.6);
     margin-top: 2em;
     padding: 1.5em 0px 0px;
     position: relative;
     top: -20px;
     width: 980px;
}

